{
  "name": "feryardiant/learn-monorepo",
  "description": "Laravel Package Template",
  "keywords": ["laravel", "package", "template"],
  "license": "MIT",
  "type": "library",
  "authors": [
    {
      "name": "Fery Wardiyanto",
      "email": "ferywardiyanto@gmail.com"
    }
  ],
  "support": {
    "source": "https://github.com/feryardiant/learn-monorepo-php"
  },
  "scripts": {
    "lint": [
      "@php vendor/bin/pint",
      "@php vendor/bin/phpstan analyse"
    ],
    "post-update-cmd": [
      "@php vendor/bin/dusk-updater detect --auto-update"
    ],
    "serve": [
      "Composer\\Config::disableProcessTimeout",
      "@php vendor/bin/testbench serve"
    ],
    "test": [
      "Composer\\Config::disableProcessTimeout",
      "@php vendor/bin/testbench package:test --ansi"
    ],
    "testbench": [
      "@php vendor/bin/testbench"
    ],
    "tinker": [
      "Composer\\Config::disableProcessTimeout",
      "@php vendor/bin/testbench tinker"
    ]
  },
  "autoload": {
    "psr-4": {
      "LearnMonorepo\\": [
        "packages/common/src",
        "packages/foo/src",
        "packages/bar/src"
      ]
    }
  },
  "autoload-dev": {
    "psr-4": {
      "LearnMonorepo\\Tests\\": [
        "packages/common/tests",
        "packages/foo/tests",
        "packages/bar/tests",
        "tests"
      ],
      "Workbench\\": "workbench"
    }
  },
  "require": {
    "laravel/framework": "^10.0|^11.0"
  },
  "require-dev": {
    "larastan/larastan": "^2.9",
    "laravel/pint": "^1.1",
    "nunomaduro/collision": "^7.10|^8.4",
    "orchestra/testbench": "^8.20|^9.4",
    "orchestra/testbench-dusk": "^9.7"
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
